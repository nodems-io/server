{
  "name": "server",
  "version": "0.0.1",
  "description": "server",
  "scripts": {
    "watch": "tsc --watch --preserveWatchOutput --pretty",
    "dev": "nodemon --watch src/ --exec npm run compile -e ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf dist && tsc",
    "compile": "tsc && node dist/index.js",
    "start": "node dist/index.js",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/data-source.ts ",
    "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts"
  },
  "author": "Recai Atak",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/formbody": "^8.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/routes": "^6.0.1",
    "@fastify/static": "^8.0.3",
    "@fastify/swagger": "^9.4.0",
    "@fastify/swagger-ui": "^5.2.0",
    "axios": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "fastify": "^5.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^10.0.0",
    "winston": "^3.15.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^18.11.18",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.11",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.8.3"
  }
}
